{
  "name": "rest-api-node",
  "version": "1.0.0",
  "description": "This is the respository to support my blog series on REST APIs with Node and Express",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "nodemon --exec npm run restart",
    "restart": "rm -rf dist && npm run build && npm run start",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvarezskinner/rest-api-node.git"
  },
  "keywords": [],
